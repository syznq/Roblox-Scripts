{
  "manifest_version": 3,
  "name": "Vezekk Bypass",
  "version": "2.0.0",
  "description": "Volcano bypasser but modified - Credits to DyRian for the Original bypasser!",
  "author": "vezekk",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "webNavigation",
    "webRequest",
    "storage"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://work.ink/*",
        "https://key.volcano.wtf/*"
      ],
      "js": ["app.js"],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}